pipeline {
    agent any

    tools {
        maven 'Maven-3.8.4'
    }

    environment {
        REPO_URL = 'https://github.com/Sukanya992/maven.git'
    }

    stages {
        stage('Checkout') {
            steps {
                git url: "${REPO_URL}", credentialsId: "${CREDENTIALS_ID}"
            }
        }

        stage('Build') {
            steps {
                sh 'mvn clean install'
            }
        }

        
    }
}
